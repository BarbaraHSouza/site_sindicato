{
    "version": 2,
    "builds": [
      {
        "src": "site_sindicato/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/statiC/(.*)",
        "dest": "/statiC/$1"
      },
      {
        "src": "/(.*)",
        "dest": "site_sindicato/wsgi.py"
      }
    ]
  }  
  